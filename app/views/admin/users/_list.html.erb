<% if @users.any? %>
  <ul>
    <% @users.each do |user| %>
      <li>
        <p>名前：<%= user.name %></p>

        <p>電話番号：<%= user.telephone_number %></p>

        <p>メールアドレス：<%= user.email %></p>

        <p>郵便番号：<%= user.postal_code %></p>

        <p>都道府県：<%= user.prefecture %></p>

        <p>市区町村：<%= user.city %></p>

        <%= link_to '詳細ページへ', admin_user_path(user.id) %>
      </li>
    <% end %>
  </ul>
<% else %>
  <p>会員が見つかりませんでした。</p>
<% end %>